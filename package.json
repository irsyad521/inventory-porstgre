        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.4.5",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.2.0",
        "nodemon": "^3.1.0",
        "pg": "^8.11.3",
        "sequelize": "^6.37.1"
    },
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.24.0",
        "@babel/preset-env": "^7.24.0",
        "babel-jest": "^29.7.0",
        "jest": "^29.7.0"
